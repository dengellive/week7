pipeline {
     agent any
     stages {
          stage("Compile") {
               steps {
                    sh "chmod +x gradlew"
                    sh "./gradlew compileJava"
               }
          }
          
          stage("Package") {
               steps {
                    sh "./gradlew build"
                    echo "Playgorund branch doesn't do much..."
               }
          } 
     }
}